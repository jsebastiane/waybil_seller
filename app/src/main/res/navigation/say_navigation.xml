<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/say_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/transactionsFragment"
        android:name="waybilmobile.company.waybil.view.transactions.TransactionsFragment"
        tools:layout="@layout/fragment_transactions"
        android:label="TransactionsFragment" >
        <action
            android:id="@+id/action_transactionsFragment_to_transactionDetails"
            app:destination="@id/transactionDetails"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/ordersFragment"
        android:name="waybilmobile.company.waybil.view.orders.OrdersFragment"
        android:label="OrdersFragment" >
        <action
            android:id="@+id/action_ordersFragment_to_orderDetailsFragment"
            app:destination="@id/orderDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/customersFragment"
        android:name="waybilmobile.company.waybil.view.customers.CustomersFragment"
        android:label="CustomersFragment" >
        <action
            android:id="@+id/action_customersFragment_to_customerDetailsFragment"
            app:destination="@id/customerDetailsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="waybilmobile.company.waybil.view.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_ordersFragment"
            app:destination="@id/ordersFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_homeFragment_to_customersFragment"
            app:destination="@id/customersFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_homeFragment_to_transactionsFragment"
            app:destination="@id/transactionsFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_homeFragment_to_inventoryFragment"
            app:destination="@id/inventoryFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/inventoryFragment"
        android:name="waybilmobile.company.waybil.view.inventory.InventoryFragment"
        android:label="InventoryFragment" >

        <action
            android:id="@+id/action_inventoryFragment_to_productFragment"
            app:destination="@id/productFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_inventoryFragment_to_addProductInventory"
            app:destination="@id/addProductInventory"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/productFragment"
        android:name="waybilmobile.company.waybil.view.product.ProductFragment"
        android:label="ProductFragment" >

        <argument
            android:name="productId"
            android:defaultValue="0"
            app:argType="string"/>

    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="waybilmobile.company.waybil.view.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_manageAccountFragment"
            app:destination="@id/manageAccountFragment"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
        <action
            android:id="@+id/action_profileFragment_to_inviteClient"
            app:destination="@id/inviteClient"
            app:enterAnim="@anim/slide_up"
            app:exitAnim="@anim/slide_down"
            app:popEnterAnim="@anim/slide_up"
            app:popExitAnim="@anim/slide_down" />
    </fragment>
    <fragment
        android:id="@+id/customerDetailsFragment"
        android:name="waybilmobile.company.waybil.view.customers.CustomerDetailsFragment"
        android:label="CustomerDetailsFragment" >

        <argument
            android:name="customerId"
            android:defaultValue="0"
            app:argType="string"/>

    </fragment>
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="waybilmobile.company.waybil.view.orders.OrderDetailsFragment"
        android:label="OrderDetailsFragment" >

        <argument
            android:name="orderId"
            android:defaultValue="0"
            app:argType="string"/>

    </fragment>
    <fragment
        android:id="@+id/addProductInventory"
        android:name="waybilmobile.company.waybil.view.inventory.AddProductInventory"
        android:label="AddProductInventory" />
    <fragment
        android:id="@+id/manageAccountFragment"
        android:name="waybilmobile.company.waybil.view.profile.ManageAccountFragment"
        android:label="ManageAccountFragment" />
    <fragment
        android:id="@+id/inviteClient"
        android:name="waybilmobile.company.waybil.view.profile.InviteClient"
        android:label="InviteClient" />
    <fragment
        android:id="@+id/transactionDetails"
        android:name="waybilmobile.company.waybil.view.transactions.TransactionDetails"
        android:label="TransactionDetails" />
</navigation>